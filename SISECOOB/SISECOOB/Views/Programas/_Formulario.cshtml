@model SISECOOB.Models.Programas


@using (@Html.BeginForm())
{
    <fieldset>
        <div class="col-md-12 col-sm-12">
            @Html.HiddenFor(i => i.ProgramaID)
            <div class=" form-group col-md-8 col-sm-8 col-md-offset-2 col-sm-offset-2">
                @Html.LabelFor(i => i.Programa)
                @Html.TextBoxFor(i => i.Programa, new { @class = "form-control" })
                @Html.ValidationMessageFor(i => i.Programa)
            </div>
        </div>
    </fieldset>
}